!!!5
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => data.page.description, :name => "description"}
    %meta{:content => "width=device-width", :name => "viewport"}
    
    %title= data.page.title
    
    //= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"
    = stylesheet_link_tag "http://netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css"

    = stylesheet_link_tag "style"
    = javascript_include_tag "site"
  %body
    %main
      %header
        //%a#show-left{:href => "#left"} Left
        %h1 Super Awesome HTML5 Community Calendar!

      %section#content
        = yield
      %footer
      #map